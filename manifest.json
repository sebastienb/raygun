{
  "manifest_version": 2,

  "name": "RayGun",
  "description": "Browser extension that allows you to kill stuff from the dom.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
 ],
 "content_scripts": [ {
        "all_frames": true,
        "js": [ "jquery.min.js", "content.js" ],
        "css":["content.css"],
        "matches": [ "http://*/*", "https://*/*" ]
  } ],
  "devtools_page": "devtools.html"
}
